		<dependency>
			<groupId>ca.bc.gov.open</groupId>
			<artifactId>jrcc-access-spring-boot-autoconfigure</artifactId>
			<version>1.3.2</version>
		</dependency>
		<dependency>
			<groupId>org.springframework.integration</groupId>
